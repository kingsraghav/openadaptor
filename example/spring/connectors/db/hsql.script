CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE TRADE(TRADEID INTEGER NOT NULL,BUYSELL CHAR(1) NOT NULL,SECID INTEGER NOT NULL,PARTYID INTEGER NOT NULL,QTY INTEGER NOT NULL,PRICE FLOAT NOT NULL)
CREATE MEMORY TABLE SECURITY(SECID INTEGER NOT NULL,SHORTCODE VARCHAR(16) NOT NULL,LONGNAME VARCHAR(64) NOT NULL)
CREATE MEMORY TABLE PARTY(PARTYID INTEGER NOT NULL,SHORTCODE VARCHAR(16) NOT NULL,LONGNAME VARCHAR(64) NOT NULL)
CREATE MEMORY TABLE TESTOUTPUT(VALUE VARCHAR(255) NOT NULL)
CREATE MEMORY TABLE TEST(TESTOUTPUT VARCHAR(255))
CREATE MEMORY TABLE OA_EXCEPTION(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,TIMESTAMP CHAR(32),EXCEPTION_CLASS_NAME CHAR(128),ORIGINATING_COMPONENT CHAR(128),DATA CHAR(256),FIXED BOOLEAN,REPROCESSED BOOLEAN)
CREATE MEMORY TABLE USER(USERID INTEGER NOT NULL,GENDER CHAR(1) NOT NULL,DOB CHAR(32))
CREATE MEMORY TABLE TRADE_STATUS(TRADEID INTEGER NOT NULL,STATUS CHAR(10) NOT NULL)
CREATE MEMORY TABLE COUNTERPARTY_ID_MAP(LONG_ID CHAR(15) NOT NULL,SHORT_ID CHAR(3) NOT NULL)
